<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:background="@color/white"

    tools:context=".YEAR3.Year3">
    <Button
        style="@style/custom_style1"
        android:id="@+id/btn_cs3"
        android:layout_marginTop="@dimen/_110sdp"
        android:text="@string/ComputerScience"
        android:background="@drawable/btn_semestar"
        />
    <Button
        style="@style/custom_style1"
        android:id="@+id/btn_ai3"
        android:background="@drawable/btn_semestar"
        android:text="@string/ArtificialIntelligence"/>
    <Button
        style="@style/custom_style1"
        android:id="@+id/btn_is3"
        android:background="@drawable/btn_semestar"
        android:text="@string/InformationSystem"/>
    <Button
        android:background="@drawable/btn_semestar"
        style="@style/custom_style1"
        android:id="@+id/btn_sc3"
        android:layout_marginBottom="@dimen/_110sdp"
        android:text="@string/ScientificComputing"/>
</LinearLayout>